{
  "name": "cordova-plugin-actito-push",
  "version": "5.0.0",
  "description": "Actito Push Cordova Plugin.",
  "author": "Actito <mobile@actito.com> (https://actito.com)",
  "license": "MIT",
  "homepage": "https://github.com/actito/actito-sdk-cordova#readme",
  "repository": {
    "url": "https://github.com/Actito/actito-sdk-cordova"
  },
  "bugs": {
    "url": "https://github.com/Actito/actito-sdk-cordova/issues"
  },
  "keywords": [
    "actito",
    "ecosystem:cordova",
    "cordova-android",
    "cordova-ios"
  ],
  "main": "build/actito-push.bundle.js",
  "types": "build/js/index.d.ts",
  "files": [
    "src",
    "build",
    "plugin.xml",
    "CHANGELOG.md",
    "MIGRATION.md"
  ],
  "scripts": {
    "build": "yarn clean; yarn build:typescript; yarn build:bundle",
    "build:typescript": "tsc",
    "build:bundle": "rollup --config",
    "clean": "rm -rf build",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "pack": "npm pack",
    "prepack": "yarn build; yarn docs:replace-symbolic-links",
    "postpack": "yarn docs:create-symbolic-links",
    "prepare": "yarn build",
    "docs:create-symbolic-links": "rm README.md CHANGELOG.md MIGRATION.md; ln -s ../../README.md ../../CHANGELOG.md ../../MIGRATION.md .",
    "docs:replace-symbolic-links": "rm README.md CHANGELOG.md MIGRATION.md; cp ../../README.md ../../CHANGELOG.md ../../MIGRATION.md ."
  },
  "peerDependencies": {
    "cordova-plugin-actito": "5.0.0"
  },
  "devDependencies": {
    "cordova-plugin-actito": "5.0.0"
  },
  "cordova": {
    "id": "cordova-plugin-actito-push",
    "platforms": [
      "android",
      "ios"
    ]
  },
  "engines": {
    "cordovaDependencies": {
      ">=3.0.0": {
        "cordova": ">= 9.0.0",
        "cordova-android": ">=8.0.0",
        "cordova-ios": ">=5.0.0"
      }
    }
  }
}
